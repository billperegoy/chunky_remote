<div class="p-8">
  <H1 class="text-2xl text-black font-bold">Create New User</h1>

  <%= form_for @changeset, Routes.user_path(@conn, :create), fn f -> %>
    <div>
      User Name <%= text_input f, :username, placeholder: "User Name" %>
    </div>

    <div>
      Email <%= text_input f, :email, placeholder: "Email" %>
    </div>

    <%= submit "Create User" %>
  <% end %>
</div>
